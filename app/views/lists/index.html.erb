

<div class="title-container">
  <h1>Create your movies list</h1>
  <button class= "btn-primary">
    <%= link_to "Add a movie list", new_list_path, class: "text-btn" %>
  </button>
</div>

<ul>
  <% @lists.each do |list| %>
   <li><%= list.name %></li>
   <span><%= link_to "See details", list_path(list) %></span>
  <% end %>
</ul>
